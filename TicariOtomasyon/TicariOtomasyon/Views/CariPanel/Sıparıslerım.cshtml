@using TicariOtomasyon.Models
@model List<SatısHareket>
@{
    ViewData["Title"] = "Sıparıslerım";
    Layout = "~/Views/Shared/CariPanel.cshtml";
}
<br />
<h1>Sipariş Geçmişi</h1>
<br />
<table class="table table-bordered">
    <tr>
        <th>Ürün Adı</th>
        <th>Fiyat</th>
        <th>Tarih</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Ürünler.UrunAd</td>
            <td>@item.Fiyat</td>
            <td>@Convert.ToDateTime(@item.Tarih).ToString("dd/mm/yy")</td>
        </tr>
    }

</table>
